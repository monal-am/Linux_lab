#include <stdio.h>
#include <ctype.h>
#include <math.h>

#define MAX 100
int stack[MAX];
int top=-1;

void push(int val){ stack[++top]=val; }
int pop(){ return stack[top--]; }

int main(){
    char postfix[100];
    printf("Enter postfix: ");
    scanf("%s",postfix);

    for(int i=0;postfix[i]!='\0';i++){
        if(isdigit(postfix[i]))
            push(postfix[i]-'0');
        else{
            int b=pop();
            int a=pop();
            switch(postfix[i]){
                case '+': push(a+b); break;
                case '-': push(a-b); break;
                case '*': push(a*b); break;
                case '/': push(a/b); break;
                case '^': push(pow(a,b)); break;
            }
        }
    }
    printf("Result = %d",pop());
}
